{
  "name": "@warpmetrics/meta-gen",
  "version": "0.1.12",
  "description": "Self-improving meta description generator with Google Search Console feedback",
  "type": "module",
  "bin": {
    "meta-gen": "./src/cli/index.js"
  },
  "exports": {
    ".": "./src/index.js",
    "./react": "./src/integrations/react-hook.js",
    "./core/*": "./src/core/*.js"
  },
  "files": [
    "src",
    "!src/**/*.test.js",
    "defaults"
  ],
  "scripts": {
    "dev": "node src/cli/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "preversion": "npm test",
    "release:patch": "npm version patch && git push origin main --tags",
    "release:minor": "npm version minor && git push origin main --tags"
  },
  "dependencies": {
    "@warpmetrics/warp": "^0.0.16",
    "chalk": "^5.4.1",
    "cheerio": "^1.2.0",
    "commander": "^12.1.0",
    "dotenv": "^16.4.7",
    "googleapis": "^144.0.0",
    "inquirer": "^12.3.0",
    "open": "^10.1.0",
    "openai": "^4.77.3",
    "ora": "^8.1.1",
    "turndown": "^7.2.2"
  },
  "keywords": [
    "seo",
    "meta-descriptions",
    "google-search-console",
    "self-improving",
    "ai"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/warpmetrics/meta-gen.git"
  },
  "homepage": "https://github.com/warpmetrics/meta-gen#readme",
  "bugs": {
    "url": "https://github.com/warpmetrics/meta-gen/issues"
  },
  "devDependencies": {
    "vitest": "^3.2.4"
  }
}
